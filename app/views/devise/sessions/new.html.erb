

<nav class="navbar2">
    <div class="navbar-header">
      <a class="navbar-brand" href="/" style="padding:0;margin-left: 30px;"><img src="../assets/dormus_logo-word-only1.png" style="width:200px;"/></a>
    </div/>
  <div class="container-fluid">
    <ul class="nav navbar-nav mynav">
       <li><a href="#">SUBMIT</a></li>
       <% if user_signed_in? %>
       <li><%= link_to "LOG OUT", destroy_user_session_path, method: :delete  %></li>
       <%else%>
       <li>
        <%= link_to('LOG IN', new_user_session_path)  %>  
        </li>
       <%end%>
    </ul>
  </div>
  </nav>
<body id="b-su">
	<section id="sign_up">
	 <div class="sign_up_form">
	   <div class="card log_form">
		   <center>
			<h2>Log In</h2>

			<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
			  <div class="form-inputs">
			    <%= f.input :email, required: false, autofocus: true %>
			    <%= f.input :password, required: false %>
			    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
			  </div>

			  <div class="form-actions">
			    <%= f.button :submit, "Log in",:class=>"btn contribute btncont"%>
			  </div>
			<% end %>
			Don't have an account yet? <%= render "devise/shared/links" %>
			</center>
		</div>
	  </div>
	</section>
</body>
